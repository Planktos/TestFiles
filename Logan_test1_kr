# operators
a <- (5:15)
a [(c(1,7))]
b <- 5; #b <- a[1]
c <- 11; #c <- a[7]
b > c
b < c
b >= c
a [c(2,6,9)]
d <- 6
f <- 10
g <- 13
((d + f) * (d + g)) / 2
10 * (f - g)

# the operator for not is NULL;
# correction question 8; the operator for negation is "!"

b %% c
# %% is for integer division
b^c
# ^ is getting the square of a number
c %/% b
# %/% is getting the remainder of the divsion
55 -> zz

q <- 34
r <- 44
s <- 54
#correction for question 12
q <- 34; r <- 44; s <- 54
v <- c(q,r,s) #this works but really I was looking for the 'print' function
print(q);print(r);print(s)

# Inf is a non-zero number divided by zero
k <- 1/0
# - Inf is a negative non-zero number divided by zero
h <- -1/0
# NaN is basically not a number
0 / 0
# Na means not available
is.na(c(1,5))

# Object Data Types
money <- "make more of it"
aa <- as.integer(222)
factor(a, money)

# Conditional Statement
animals <- c("dog","penguin","horse","lion")
animals
if("dog" %in% animals){
  print (TRUE)
} else {
  print (FALSE)
}
